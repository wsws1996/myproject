	public static <T> T request2Bean(HttpServletRequest request, Class<T> clazz) {
		try {
			T bean = clazz.newInstance();
			Enumeration enumeration= request.getParameterNames();
		} catch (Exception e) {
			// TODO: handle exception
		}
		return null;
	}
}
